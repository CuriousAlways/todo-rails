<div class="container">
  <%= form_with url: '/login' do |form| %>
    <div>
      <%= form.label :email %>
      <%= form.email_field :email %>
    </div>

    <div>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>

    <div>
      <%= form.submit 'Login' %>
    </div>
  <% end %>
</div>
